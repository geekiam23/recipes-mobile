var _curry2 = require("./internal/_curry2");

var equals = require("./equals");

var map = require("./map");

var where = require("./where");

var whereEq = _curry2(function whereEq(spec, testObj) {
  return where(map(equals, spec), testObj);
});

module.exports = whereEq;
