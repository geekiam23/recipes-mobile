var _curry2 = require("./internal/_curry2");

var _dispatchable = require("./internal/_dispatchable");

var _xfind = require("./internal/_xfind");

var find = _curry2(_dispatchable(['find'], _xfind, function find(fn, list) {
  var idx = 0;
  var len = list.length;

  while (idx < len) {
    if (fn(list[idx])) {
      return list[idx];
    }

    idx += 1;
  }
}));

module.exports = find;
