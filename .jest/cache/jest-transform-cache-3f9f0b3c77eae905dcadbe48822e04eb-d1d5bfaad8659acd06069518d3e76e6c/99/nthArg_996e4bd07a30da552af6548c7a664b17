var _curry1 = require("./internal/_curry1");

var curryN = require("./curryN");

var nth = require("./nth");

var nthArg = _curry1(function nthArg(n) {
  var arity = n < 0 ? 1 : n + 1;
  return curryN(arity, function () {
    return nth(n, arguments);
  });
});

module.exports = nthArg;
